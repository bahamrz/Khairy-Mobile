<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="windowActionBar">true</item>
        <item name="windowNoTitle">false</item>
    </style>
    <style name="AppThemeKhairyLogin.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar"/>
    <style name="AppThemeKhairyLogin.PopupOverlay" parent="ThemeOverlay.AppCompat.Light"/>

    <!--    Base Khairy Theme -->
    <style name="AppThemeKhairyRegist" parent="Theme.AppCompat.Light.DarkActionBar">
        <!--    Here is the Theme For Khairy -->
        <item name="colorPrimary" type="color">@color/colorPrimar</item>
        <item name="colorPrimaryDark" type="color">@color/colorPrimaryDart</item>
        <item name="colorAccent" type="color">@color/colorAccent</item>
    </style>

    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar"/>
    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light"/>

</resources>
